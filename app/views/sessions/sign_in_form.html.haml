= simple_form_for(@form, url: sessions_sign_in_path, as: :session) do |f|
  = f.error_notification

  = f.input :email
  = f.input :password

  = f.button :submit, "Se connecter", class: "btn-primary btn-block"

.mastfoot
  .inner
    .text-center
      = link_to "Inscription", sessions_sign_up_form_path


